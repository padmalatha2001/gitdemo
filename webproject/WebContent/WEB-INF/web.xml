<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <servlet>
        <servlet-name>sample</servlet-name>
        <servlet-class>webproject.MyServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>sample</servlet-name>
        <url-pattern>/MyServlet</url-pattern>
    </servlet-mapping>
    
     <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>webproject.LoginPage</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/LoginPage</url-pattern>
    </servlet-mapping>
    
     <servlet>
        <servlet-name>loginwebout</servlet-name>
        <servlet-class>webproject.WebLogin</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>loginwebout</servlet-name>
        <url-pattern>/WebLogin</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>userregistration</servlet-name>
        <servlet-class>webproject.UserRegistration</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>userregistration</servlet-name>
        <url-pattern>/UserRegistration</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>VerifyLogin</servlet-name>
        <servlet-class>webproject.VerifyLogin</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>VerifyLogin</servlet-name>
        <url-pattern>/VerifyLogin</url-pattern>
    </servlet-mapping>
    
</web-app>